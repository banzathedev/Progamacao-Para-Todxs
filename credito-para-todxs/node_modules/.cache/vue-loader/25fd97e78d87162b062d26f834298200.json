{"remainingRequest":"C:\\Users\\grapo\\Documents\\GitHub\\Prog-Para-Todx\\credito-para-todxs\\node_modules\\vue-loader\\lib\\index.js??vue-loader-options!C:\\Users\\grapo\\Documents\\GitHub\\Prog-Para-Todx\\credito-para-todxs\\src\\components\\views\\Buying.vue?vue&type=script&lang=js&","dependencies":[{"path":"C:\\Users\\grapo\\Documents\\GitHub\\Prog-Para-Todx\\credito-para-todxs\\src\\components\\views\\Buying.vue","mtime":1624018436309},{"path":"C:\\Users\\grapo\\Documents\\GitHub\\Prog-Para-Todx\\credito-para-todxs\\node_modules\\cache-loader\\dist\\cjs.js","mtime":499162500000},{"path":"C:\\Users\\grapo\\Documents\\GitHub\\Prog-Para-Todx\\credito-para-todxs\\node_modules\\babel-loader\\lib\\index.js","mtime":315532800000},{"path":"C:\\Users\\grapo\\Documents\\GitHub\\Prog-Para-Todx\\credito-para-todxs\\node_modules\\cache-loader\\dist\\cjs.js","mtime":499162500000},{"path":"C:\\Users\\grapo\\Documents\\GitHub\\Prog-Para-Todx\\credito-para-todxs\\node_modules\\vue-loader\\lib\\index.js","mtime":499162500000}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["Buying.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA0LA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"Buying.vue","sourceRoot":"src/components/views","sourcesContent":["<template>\r\n  <div>\r\n    <div>\r\n      <form @submit.prevent=\"submit\">\r\n        <div>\r\n          <v-text-field\r\n            v-model=\"formInformation.name\"\r\n            label=\"Nome Completo\"\r\n            hint=\"ex: João Aparecido da Silva\"\r\n            required\r\n          ></v-text-field>\r\n          <v-text-field\r\n            v-model=\"formInformation.cpf\"\r\n            label=\"Cpf\"\r\n            hint=\"Siga o padrão ex: 12312312300 (cpf deve ser inserido sem espaçamento ou caracteres)\"\r\n            type=\"number\"\r\n            required\r\n          ></v-text-field>\r\n          <v-text-field\r\n            v-model=\"formInformation.phoneNumber\"\r\n            label=\"Numero de contato\"\r\n            hint=\"Siga o padrão ex: 11 9 99009900 (Sem espaços e apenas numeros)\"\r\n            type=\"number\"\r\n            required\r\n          ></v-text-field>\r\n          <v-text-field\r\n            v-model=\"formInformation.email\"\r\n            label=\"E-mail\"\r\n            hint=\"Siga o Padrão ex: MeuEmail@meuemail.com.br\"\r\n            required\r\n          ></v-text-field>\r\n        </div>\r\n        <br />\r\n        <div>\r\n          <p\r\n            v-for=\"chosedProposal in proposalForDialog\"\r\n            :key=\"chosedProposal.name\"\r\n          >\r\n            selecionado: {{ chosedProposal.name }}\r\n          </p>\r\n          <v-checkbox required color=\"green\">\r\n            <template v-slot:label>\r\n              <div>\r\n                Do you accept the\r\n                <a href=\"#\">terms</a>\r\n                and\r\n                <a href=\"#\">conditions?</a>\r\n              </div>\r\n            </template>\r\n          </v-checkbox>\r\n        </div>\r\n        <div>\r\n          <v-dialog\r\n            transition=\"dialog-bottom-transition\"\r\n            max-height=\"50%\"\r\n            max-width=\"50%\"\r\n          >\r\n            <template v-slot:activator=\"{ on, attrs }\">\r\n              <v-btn v-bind=\"attrs\" v-on=\"on\" class=\"mr-4\"> revisar </v-btn>\r\n            </template>\r\n            <template v-slot:default=\"dialog\">\r\n              <v-card>\r\n                <v-toolbar color=\"primary\" dark>Revisão de dados</v-toolbar>\r\n                <v-card-text>\r\n                  <div>\r\n                    <v-card-text>\r\n                      <v-card-title class=\"titles mb-3\"\r\n                        >Informações pessoais</v-card-title\r\n                      >\r\n                      <v-simple-table height=\"150px\">\r\n                        <thead>\r\n                          <tr>\r\n                            <th class=\"text-h6\">Nome</th>\r\n                            <th class=\"text-h6\">Numero para Contato</th>\r\n                            <th class=\"text-h6\">Cpf</th>\r\n                            <th class=\"text-h6\">Email</th>\r\n                          </tr>\r\n                        </thead>\r\n                        <tbody>\r\n                          <tr>\r\n                            <td class=\"text-body-1\">\r\n                              {{ formInformation.name }}\r\n                            </td>\r\n                            <td class=\"text-body-1\">\r\n                              {{ formInformation.phoneNumber }}\r\n                            </td>\r\n                            <td class=\"text-body-1\">\r\n                              {{ formInformation.cpf }}\r\n                            </td>\r\n                            <td class=\"text-body-1\">\r\n                              {{ formInformation.email }}\r\n                            </td>\r\n                          </tr>\r\n                        </tbody>\r\n                      </v-simple-table>\r\n                    </v-card-text>\r\n                    <v-card-text\r\n                      v-for=\"chosedProposal in proposalForDialog\"\r\n                      :key=\"chosedProposal.name\"\r\n                    >\r\n                      <v-card-title class=\"titles mb-4\"\r\n                        >Informações do Emprestimo</v-card-title\r\n                      >\r\n                      <v-simple-table height=\"150px\">\r\n                        <thead>\r\n                          <tr>\r\n                            <th class=\"text-h6\">Proposta escolhida</th>\r\n                            <th class=\"text-h6\">Valor de empréstimo</th>\r\n                            <th class=\"text-h6\">Qtnd. Parcelas</th>\r\n                            <th class=\"text-h6\">Juros Ao Mes</th>\r\n                            <th class=\"text-h6\">Valor das Parcelas</th>\r\n                          </tr>\r\n                        </thead>\r\n                        <tbody>\r\n                          <tr>\r\n                            <td class=\"text-body-1\">\r\n                              {{ chosedProposal.name }}\r\n                            </td>\r\n                            <td class=\"text-body-1\">\r\n                              {{ chosedProposal.valorEmprestimo }}\r\n                            </td>\r\n                            <td class=\"text-body-1\">\r\n                              {{ chosedProposal.parcelas }}x\r\n                            </td>\r\n                            <td class=\"text-body-1\">\r\n                              {{ chosedProposal.juros * 100 }}%\r\n                            </td>\r\n                            <td class=\"text-body-1\">\r\n                              {{\r\n                                (chosedProposal.valorEmprestimo /\r\n                                  chosedProposal.parcelas +\r\n                                  chosedProposal.valorEmprestimo *\r\n                                    chosedProposal.juros)\r\n                                  | dinheiro\r\n                              }}\r\n                            </td>\r\n                          </tr>\r\n                        </tbody>\r\n                      </v-simple-table>\r\n                    </v-card-text>\r\n                  </div>\r\n                </v-card-text>\r\n                <v-footer>\r\n                  <v-card-actions class=\"justify-end\">\r\n                    <v-btn text @click=\"dialog.value = false\"\r\n                      >voltar a tabela</v-btn\r\n                    >\r\n                    <v-btn\r\n                      text\r\n                      @click=\"dialog.value = false\"\r\n                      @mousedown=\"revise\"\r\n                      >confirmar</v-btn\r\n                    >\r\n                  </v-card-actions>\r\n                </v-footer>\r\n              </v-card>\r\n            </template>\r\n          </v-dialog>\r\n          <v-btn @click.stop=\"sendform\" :disabled=\"disable\"> Contratar </v-btn>\r\n        </div>\r\n      </form>\r\n    </div>\r\n    <div>\r\n      <v-alert\r\n        @mousedown=\"goToHome\"\r\n        :value=\"alert\"\r\n        dismissible\r\n        border=\"bottom\"\r\n        colored-border\r\n        type=\"info\"\r\n        elevation=\"2\"\r\n        icon=\"mdi-home\"\r\n        transition=\"scale-transition\"\r\n        hint=\"burrão\"\r\n      >\r\n        <p class=\"p-alerts\">\r\n          Um email foi enviado para voce confirmando sua escolha, em breve um de\r\n          nossos atendentes entrará em contato por meio dos dados fornecidos\r\n        </p>\r\n        <p class=\"p-alerts2\">pressione X para retornar a pagina inícial</p>\r\n      </v-alert>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { mapActions, mapGetters } from \"vuex\";\r\n\r\nexport default {\r\n  data: () => ({\r\n    formInformation: {\r\n      cpf: \"\",\r\n      name: \"\",\r\n      phoneNumber: \"\",\r\n      email: \"\",\r\n    },\r\n    disable: true,\r\n    //checkbox: null,\r\n    alert: false,\r\n  }),\r\n\r\n  methods: {\r\n    ...mapActions([\"sendedformData\"]),\r\n\r\n    sendform() {\r\n      const formData = this.formInformation;\r\n      this.sendedformData(formData);\r\n      this.alert = true;\r\n    },\r\n    revise() {\r\n      this.disable = false;\r\n      return console.log(\"chamou revisão\");\r\n    },\r\n    goToHome() {\r\n      return this.$router.push(\"/\");\r\n    },\r\n  },\r\n  computed: {\r\n    ...mapGetters([\"setchosedProposal\"]),\r\n\r\n    proposalForDialog() {\r\n      return this.setchosedProposal[0];\r\n    },\r\n  },\r\n  created() {\r\n    this.proposalForDialog;\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.titles {\r\n  justify-content: center;\r\n  font-size: 2rem;\r\n  font-weight: 400;\r\n}\r\n.p-alerts {\r\n  justify-content: flex-start;\r\n  font-size: 1.5rem;\r\n  font-weight: 400;\r\n}\r\n.p-alerts2 {\r\n  justify-content: center;\r\n  font-size: 1.5rem;\r\n  font-weight: 600;\r\n}\r\n</style>"]}]}